# Senate CWC Worker Lambda Environment Configuration
# Copy this file to .env and fill in your actual values

# CWC API Configuration
CWC_API_KEY=your-senate-cwc-api-key-here
CWC_API_BASE_URL=https://soapbox.senate.gov/api

# AWS Configuration
DYNAMO_TABLE_NAME=communique-rate-limits-production
AWS_REGION=us-east-1

# Communique Integration
JOB_STATUS_API_URL=https://app.communique.com
LAMBDA_WEBHOOK_SECRET=your-webhook-secret-here

# Rate Limiting Configuration
MAX_RETRIES=3
RATE_LIMIT_WINDOW_SECONDS=3600
RATE_LIMIT_COUNT=10
VISIBILITY_TIMEOUT_SECONDS=300

# Development/Testing
NODE_ENV=production
LOG_LEVEL=info